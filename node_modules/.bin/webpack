#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/laoluo/myCodeSet/webpack_test/node_modules/.pnpm/webpack@4.46.0/node_modules/webpack/bin/node_modules:/Users/laoluo/myCodeSet/webpack_test/node_modules/.pnpm/webpack@4.46.0/node_modules/webpack/node_modules:/Users/laoluo/myCodeSet/webpack_test/node_modules/.pnpm/webpack@4.46.0/node_modules:/Users/laoluo/myCodeSet/webpack_test/node_modules/.pnpm/node_modules:/Users/laoluo/myCodeSet/webpack_test/node_modules:/Users/laoluo/myCodeSet/node_modules:/Users/laoluo/node_modules:/Users/node_modules:/node_modules:/Users/laoluo/myCodeSet/webpack_test/node_modules/webpack/bin/node_modules:/Users/laoluo/myCodeSet/webpack_test/node_modules/webpack/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../webpack/bin/webpack.js" "$@"
else
  exec node  "$basedir/../webpack/bin/webpack.js" "$@"
fi
